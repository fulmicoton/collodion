LIVEPTN:=(?<LIVEPTN.NAME>.)(?<LIVEVB>[LIVE])
SALARY:=[DOLLAR](?<SALARY.MIN>[NUMBER])[TO][DOLLAR](?<SALARY_MAX>[NUMBER])
